msgctxt "Languages::text_emerchantpay_notice"
msgid "Please add these URL fields in your eMerchantPay eCommerce Payment Form configuration:<br/><br/><b>BackReturn URL:</b><br/>[backreturn_url]<br/><br/><b>Custom Approval URL:</b><br/>[notify_url]<br/><br/><b>Custom Decline URL:</b><br/>[decline_url]<br/><br/><b>Activate Notification URL:</b><br/>[process_url]"
msgstr "Xin vui lòng thêm các trường URL trong eMerchantPay eCommerce Payment Form của bạn: <br/> <br/> <b>BackReturn URL:</b> <br/>[backreturn_url] <br/> <br/> <b>Custom Approval URL:</b> <br/>[notify_url] <br/> <br/> <b>Custom Decline URL:</b> <br/>[decline_url] <br/> <br/> <b>Activate Notification URL:</b> <br/>[process_url]"

msgctxt "Languages::text_enets_notice"
msgid "Please send the following URL to the eNPS manager as a response URL for all the three cases (success, failure, cancel): [r_url]"
msgstr "Xin vui lòng gửi URL sau đây cho người quản lý eNPS là một phản ứng URL cho tất cả các trường hợp ba (thành công, thất bại, hủy bỏ): [r_url]"

msgctxt "Languages::text_enter_filename"
