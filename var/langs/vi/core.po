msgctxt "Languages::text_paysitecash_notice"
msgid "Please add these URL fields in your Paysite Cash configuration:<br/><br/><b>URL:</b><br/>[url]<br/><br/><b>Referer url:</b><br/>[ref_url]<br/><br/><b>after payment return url:</b><br/>[sucess_url]<br/><br/><b>canceled payment url:</b><br/>[cancel_url]<br/><br/><b>Backoffice confirmation url:</b><br/>[confirm_url]"
msgstr "Xin vui lòng thêm các trường URL trong cấu hình Paysite Cash của bạn: <br/> <br/> <b>URL:</b> <br/>[url] <br/> <br/> <b>Referer url:</b> <br/>[ref_url] <br/> <br/> <b>url sau khi thanh toán trả lại:</b> <br/>[sucess_url] <br/> <br/> <b>url hủy bỏ thanh toán:</b> <br/>[cancel_url] <br/> <br/> <b>Backoffice xác nhận url:</b> <br/>[confirm_url]"

msgctxt "Languages::text_paysitecash_processor"
msgid "Processor"
msgstr "Xử lý"

msgctxt "Languages::text_paysitecash_site_id"
msgid "Site ID"
msgstr "ID Trang web"

msgctxt "Languages::text_payway_notice"
msgid "Please set up the following parameters in PayWay back end: <p> Browser Return URL Pre Payment: <b>[prepayment]</b><br /> Browser Return URL: <b>[return]</b><br /> Notification URL: <b>[notify]</b><br /> Notification Post Type: leave empty </p>"
msgstr "Xin vui lòng thiết lập các thông số sau trong quản trị PayWay: <p>Browser Return URL Pre Payment: <b>[prepayment]</b> <br />Browser Return URL: <b>[return]</b> <br />Notification URL: <b>[notify]</b> <br /> Notification Post Type: để trống</p>"

msgctxt "Languages::text_permissions_changed"
msgid "Permissions changed"
msgstr "Cho phép thay đổi"

msgctxt "Languages::text_piraeus_notice"
msgid "<b>Note:</b> Please send the following information to the payment support:<br /> Website URL: '<u>[website_url]</u>'<br /> Referrer URL: '<u>[referrer_url]</u>'<br /> Success URL: '<u>[success_url]</u>'<br /> Failure URL: '<u>[failure_url]</u>'<br /> Backlink URL: '<u>[backlink_url]</u>'<br /> IP Address: '<u>[ip_address]</u>'<br /><br /> Response Method: '<u>[response_method]</u>'<br /><br />"
msgstr "<b>Lưu ý:</b> Xin vui lòng gửi các thông tin sau để hỗ trợ thanh toán: <br /> URL của trang web: ' <u>[website_url]'</u> <br /> Referrer URL: ' <u>[referrer_url]'</u> <br /> URL thành công: ' <u>[success_url]'</u> <br /> URL thất bại: ' <u>[failure_url]'</u> <br /> Backlink URL: ' <u>[backlink_url]'</u> <br /> Địa chỉ IP: ' <u>[ip_address]'</u> <br /> <br /> Phương thức phản hồi: ' <u>[response_method]'</u> <br /> <br />"

msgctxt "Languages::text_position_updating"
msgid "Loading... Positions are being updated, please wait."
msgstr "Đang tải... Vị trí đang được cập nhật, xin vui lòng chờ."

msgctxt "Languages::text_prettyphoto"
msgid "prettyPhoto"
msgstr "prettyPhoto"

msgctxt "Languages::text_price_dec_sign_delimiter"
msgid "Decimal separator for product prices."
msgstr "Dấu thập phân phân tách cho giá sản phẩm."

msgctxt "Languages::text_products_added"
msgid "The products have been added successfully. See the list of the new products below."
msgstr "Các sản phẩm đã được thêm vào thành công. Xem danh sách các sản phẩm mới dưới đây."

msgctxt "Languages::text_products_cloned"
msgid "The products have been cloned successfully. Below the list of new products"
msgstr "Các sản phẩm đã được sao chép thành công. Dưới đây là danh sách các sản phẩm mới"

msgctxt "Languages::text_products_have_been_deleted"
msgid "Products have been deleted successfully."
msgstr "Sản phẩm đã được xóa thành công."

msgctxt "Languages::text_products_updated"
msgid "The following products have been updated:"
msgstr "Các sản phẩm sau đây đã được cập nhật:"

msgctxt "Languages::text_products_updated_successfully"
msgid "The product(s) have been updated successfully"
msgstr "Các sản phẩm đã được cập nhật thành công"

msgctxt "Languages::text_product_cloned"
msgid "The product has been cloned"
msgstr "Sản phẩm đã được sao chép"

msgctxt "Languages::text_product_detailed_image"
msgid "(optional displayed in popup window)"
msgstr "(tùy chọn hiển thị trong cửa sổ popup)"

msgctxt "Languages::text_product_file_has_been_deleted"
msgid "Product file has been deleted"
msgstr "Tập tin sản phẩm đã bị xóa"

msgctxt "Languages::text_product_filters_were_disabled"
msgid "The following <a href=\"[url]\" target=\"_blank\">product filters</a> were disabled: [filters_list]."
msgstr "Mục <a href=\"[url]\" target=\"_blank\"> bộ lọc sản phẩm</a> đã được vô hiệu hóa: [filters_list]."

msgctxt "Languages::text_product_has_been_deleted"
msgid "Product has been deleted successfully."
msgstr "Sản phẩm đã được xóa thành công."

msgctxt "Languages::text_product_thumbnail"
msgid "(displayed on products list and product details pages)"
msgstr "(hiển thị trên danh sách sản phẩm và các trang chi tiết sản phẩm)"

msgctxt "Languages::text_profile_activated"
msgid "Your account is now active. You may log in to your account using your credentials."
msgstr ""

msgctxt "Languages::text_profile_benefits"
msgid "<h4>Benefits of becoming a registered member</h4><ul><li>Log in at any time to check order statuses</li><li>Personalize your shopping</li><li>Speed up future purchases</li></ul>"
msgstr "<h4>Lợi ích của việc trở thành một thành viên đăng ký</h4> <ul><li>Đăng nhập vào bất kỳ lúc nào để kiểm tra trạng thái đơn đặt hàng</li> <li>Cá nhân hoá việc mua sắm của bạn</li> <li>Mua hàng và thanh toán nhanh hơn trong lần tiếp theo</li></ul>"

msgctxt "Languages::text_profile_details"
msgid "<h4>Profile details</h4><p>On this page you can modify your login credentials and personal data to be used during future purchases.</p><p>To keep your account secure we recommend to avoid creating passwords that use:</p><ul><li>Dictionary words in any language.</li><li>Words spelled backwards, common misspellings, and abbreviations.</li><li>Sequences or repeated characters. Examples: 12345678, 222222, abcdefg, or adjacent letters on your keyboard (qwerty).</li><li>Personal information. Your name, birthday, driver's license, passport number, or similar information.</li></ul>"
msgstr "<h4>Hồ sơ chi tiết</h4> <p>Trên trang này bạn có thể sửa đổi thông tin đăng nhập và các dữ liệu cá nhân được sử dụng trong để mua hàng.</p> <p>Để giữ cho tài khoản của bạn bảo mật an toàn chúng tôi khuyên bạn nên tránh việc tạo mật khẩu sử dụng:</p> <ul><li>Từ điển trong ngôn ngữ bất kỳ.</li> <li>Từ đánh vần ngược, lỗi chính tả phổ biến, và chữ viết tắt.</li> <li>Trình tự hoặc các ký tự lặp đi lặp lại. Ví dụ: 12345678, 222222, abcdefg, hoặc các chữ cái liền kề trên bàn phím (qwerty).</li> <li>Thông tin cá nhân. Tên của bạn, sinh nhật, số giấy phép lái xe, số hộ chiếu, hoặc thông tin tương tự.</li></ul>"

msgctxt "Languages::text_profile_is_created"
msgid "The account has been created successfully."
msgstr "Tài khoản đã được tạo thành công."

msgctxt "Languages::text_profile_is_updated"
msgid "The profile data has been updated successfully."
msgstr "Dữ liệu hồ sơ đã được cập nhật thành công."

msgctxt "Languages::text_profile_should_be_approved"
msgid "Your account has been created successfully, but it must be activated by the store administrator before you are able to sign in."
msgstr "Tài khoản của bạn đã được tạo thành công, nhưng nó phải được kích hoạt bởi các quản trị viên cửa hàng trước khi bạn có thể đăng nhập."

msgctxt "Languages::text_promotions_group_condition"
msgid "if [set] of these conditions are [set_value]"
msgstr "Nếu [set] của những điều kiện này là [set_value]"

msgctxt "Languages::our_brands"
msgid "Our Brands"
msgstr "Thương hiệu của chúng tôi"

msgctxt "Languages::text_qty_discounts"
msgid "Our quantity discounts"
msgstr "Số lượng giảm giá của chúng tôi"

msgctxt "Languages::text_recover_password"
msgid "<p>If you have forgotten your password, enter your email address in the field and click <i>Reset password</i>.</p><p>You will receive a new password and a link to sign in. You will be able to change the password later.</p>"
msgstr "<p>Nếu bạn đã quên mật khẩu của bạn, nhập địa chỉ email vào trường và bấm vào <i>Đặt lại mật khẩu</i>.</p> <p>Bạn sẽ nhận được một mật khẩu mới và một liên kết để đăng nhập. Sau đó bạn có thể thay đổi mật khẩu.</p>"

msgctxt "Languages::text_recover_password_notice"
msgid "Enter your email address to receive a new login key and a link to sign in and change your password."
msgstr "Nhập địa chỉ email để nhận được một mã khóa đăng nhập mới cùng với một liên kết để đăng nhập và thay đổi mật khẩu của bạn."

msgctxt "Languages::text_recover_password_title"
msgid "Reset password"
msgstr "Đặt lại mật khẩu"

msgctxt "Languages::text_redactor"
msgid "Redactor"
msgstr "Redactor"

msgctxt "Languages::text_remove_additional_images"
msgid "When importing additional images of an updated product, existing images of this product will be deleted before import"
msgstr "Khi nhập dữ liệu hình ảnh bổ sung của một sản phẩm cập nhật, các hình ảnh sẵn có của sản phẩm này sẽ bị xóa trước khi nhập"

msgctxt "Languages::text_remove_backup_files"
msgid "Warning! If you remove the backup files, you will not be able to restore your previous installation. Ensure that the new version is working properly, and the backup database files are in a safe place."
msgstr "Cảnh báo! Nếu bạn loại bỏ các tập tin sao lưu, bạn sẽ không thể khôi phục lại cài đặt trước của bạn. Đảm bảo rằng các phiên bản mới đang làm việc đúng, và các tập tin sao lưu cơ sở dữ liệu ở một nơi an toàn."

msgctxt "Languages::text_required_group_product"
msgid "Please select a product for the required group [group_name]"
msgstr "Vui lòng chọn một sản phẩm cho nhóm yêu cầu [group_name]"

msgctxt "Languages::text_reset_inventory_description"
msgid "Quantity of all products in the store will be reset."
msgstr "Số lượng của tất cả các sản phẩm trong các cửa hàng sẽ được đặt lại."

msgctxt "Languages::text_restore_question"
msgid "Are you sure that you want to restore the template?"
msgstr "Bạn có chắc rằng bạn muốn khôi phục các mẫu giao diện không?"

msgctxt "Languages::text_sagepay_dir_notice"
msgid "<b>Note:</b> In order to track your SagePay orders by the shopping cart software, please enable the 3D Secure Fraud Checking Option for your SagePay VSP Direct account."
msgstr "<b>Lưu ý:</b> Để theo dõi các đơn đặt hàng SagePay bởi phần mềm giỏ hàng, xin vui lòng kicha hoạt 3D Secure Fraud Checking Option cho tài khoản SagePay VSP Direct của bạn."

msgctxt "Languages::text_sagepay_notice"
msgid "Use <b>testvendor</b> as <b>Vendor Name</b> for the testing purposes<br /><p><b>PAYMENT</b> - transaction type is used to gain an authorisation from the bank, then settle that transaction early the following morning, committing the funds to be taken from your customer's card.<br /></p><p><b>DEFERRED</b> - transactions are NOT sent to the bank for completion until you RELEASE them by logging into the SagePay VSP Admin interface, finding the transaction and clicking the Release button.</p>"
msgstr "Sử dụng <b>testvendor</b> như là <b>Tên Nhà cung cấp</b> cho mục đích thử nghiệm <br /> <p><b>THANH TOÁN</b> - loại giao dịch được sử dụng để đạt được một ủy quyền từ ngân hàng, sau đó giải quyết mà giao dịch sáng sớm sau, cam kết các khoản tiền sẽ được thực hiện từ thẻ của khách hàng <br /></p> <p><b>DEFERRED</b> – giao dịch sẽ không được gửi đến ngân hàng để hoàn thành cho đến khi bạn thả chúng bằng cách đăng nhập vào giao diện SagePay VSP Admin, việc tìm kiếm các giao dịch và nhấp vào nút phát hành.</p>"

msgctxt "Languages::text_secret_key_notice"
msgid "Please complete this field with any non-empty value. It will increase the security of transactions."
msgstr "Xin vui lòng hoàn thành trường này với bất kỳ giá trị không để trống nào. Nó sẽ làm tăng tính bảo mật của các giao dịch."

msgctxt "Languages::text_select_fields2edit_note"
msgid "You are able to modify several items at once. Select the check boxes corresponding to the fields you wish to edit, and click on Modify Selected."
msgstr "Bạn có thể sửa đổi nhiều mục cùng một lúc. Chọn hộp kiểm tương ứng với các mục mà bạn muốn chỉnh sửa và nhấp vào nút Sửa Mục Được Chọn."

msgctxt "Languages::text_select_file"
msgid "Select a file or enter a URL"
msgstr "Chọn một tập tin hoặc nhập một URL"

msgctxt "Languages::text_select_range"
msgid "You can select a range to export (otherwise all [name] will be exported)"
msgstr "Bạn có thể chọn một phạm vi để xuất dữ liệu (nếu không tất cả [name] sẽ được xuất dữ liệu)"

msgctxt "Languages::text_share_product_features_tooltip"
msgid "Attention! Some data will not be copied during this operation: the categories set in filters will be lost even if you copy all categories. Please, fix categories manually."
msgstr "Chú ý! Một số dữ liệu sẽ không được sao chép trong thao tác này: các thiết lập danh mục trong bộ lọc sẽ bị mất ngay cả khi bạn sao chép tất cả các danh mục. Xin vui lòng, sửa danh mục theo cách thủ công."

msgctxt "Languages::text_share_product_filters_tooltip"
msgid "Attention! Some data will not be copied during this operation: the categories set in features will be lost even if you copy all categories. Please, fix categories manually."
msgstr "Chú ý! Một số dữ liệu sẽ không được sao chép trong thao tác này: các thiết lập danh mục trong các tính năng sẽ bị mất ngay cả khi bạn sao chép tất cả các danh mục. Xin vui lòng, sửa danh mục theo cách thủ công."

msgctxt "Languages::text_share_promotions_tooltip"
msgid "In order to share promotions make sure the respective products are also shared (check Share products below)"
msgstr "Đơn hàng để chia sẻ chương trình khuyến mãi chắc chắn rằng các sản phẩm tương ứng cũng được chia sẻ (kiểm tra chia sẻ sản phẩm dưới đây)"

msgctxt "Languages::text_shipping_packages_info"
msgid "To ensure a more precise shipping rate estimation, the products have been separated into packages as defined by the product settings and the shipping method configuration. The following info is for referential use only"
msgstr "Để đảm bảo một chi tiết mức phí vận chuyển chính xác, các sản phẩm đã được tách ra thành các gói theo chỉ định của cài đặt sản phẩm và cấu hình phương pháp vận chuyển. Các thông tin sau đây chỉ sử dụng để tham khảo"

msgctxt "Languages::text_shipping_rates_changed"
msgid "Shipping rates were changed"
msgstr "Phí vận chuyển đã được thay đổi"

msgctxt "Languages::text_ship_to_billing"
msgid "Order will be delivered to the billing address"
msgstr "Đơn hàng sẽ được gửi đến địa chỉ thanh toán"

msgctxt "Languages::text_skrill_activate_quick_checkout_short_explanation"
msgid "Skrill Merchant Tools enables you to take payments from credit cards, debit cards and over 60 other local payment options in over 200 countries. The highly competitive rates for this service are published on the Skrill website at www.skrill.com."
msgstr "Skrill Merchant Tools cho phép bạn có một khoản thanh toán từ thẻ tín dụng, thẻ ghi nợ và hơn 60 các tùy chọn thanh toán địa phương khác ở hơn 200 quốc gia. Các mức giá rất cạnh tranh cho dịch vụ này được công bố trên trang web Skrill tại www.skrill.com."

msgctxt "Languages::text_skrill_activate_quick_checkout_short_explanation_1"
msgid "You have sent a request for activation on the [date]. Please be aware that the verification process to use Skrill Merchant Tools could take up to 72 hours. You will be contacted by Skrill when the verification process has been completed."
msgstr "Bạn có gửi một yêu cầu để kích hoạt [date]. Vui lòng lưu ý rằng quá trình xác minh để sử dụng Skrill Merchant Tools có thể mất đến 72 giờ. Bạn sẽ được Skrill liên hệ khi quá trình xác minh đã được hoàn thành."

msgctxt "Languages::text_skrill_activate_quick_checkout_short_explanation_2"
msgid "After activation Skrill will grant you access to a new section in your Skrill Account called Merchant Tools. There please choose a secret word (NOT the same as your password) and enter it in the section below to connect to Skrill. The secret word is the last step of your activation process and encrypts your payments securely. After successful submission you will be ready to use all the direct payment options of Skrill."
msgstr "Sau khi kích hoạt Skrill sẽ cấp quyền truy cập vào một phần mới trong Tài khoản Skrill gọi là Merchant Tools (Công cụ Thương gia). Hãy chọn một từ bí mật (không giống như mật khẩu của bạn) và nhập nó trong phần dưới đây để kết nối với Skrill. Từ bí mật là bước cuối cùng trong quá trình kích hoạt của bạn và mã hóa các khoản thanh toán của bạn một cách an toàn. Sau khi thành công trình, bạn sẽ được sẵn sàng để sử dụng tất cả các tùy chọn thanh toán trực tiếp của Skrill."

msgctxt "Languages::text_skrill_currs_notice"
msgid "If you selected a currency that is not the base one in your store, please make sure that it has the correct <a href=\"http://en.wikipedia.org/wiki/ISO_4217\">ISO 4217 code</a> on the <a href=\"[link]\">currencies</a> page."
msgstr "Nếu bạn đã chọn một loại tiền tệ mà không phải là cơ sở trong cửa hàng của bạn, hãy chắc chắn rằng nó đã đúng <a href=\"http://en.wikipedia.org/wiki/ISO_4217\"> mã ISO 4217</a> trên trang <a href=\"[link]\">tiền tệ</a>."

msgctxt "Languages::text_skrill_email_is_not_registered"
msgid "Specified email address is not registered in Skrill."
msgstr "Địa chỉ email đã chỉ định không được đăng ký tại Skrill."

msgctxt "Languages::text_skrill_email_is_registered"
msgid "Specified email address is registered in Skrill."
msgstr "Địa chỉ email chỉ định đã được đăng ký tại Skrill."

msgctxt "Languages::text_skrill_empty_input_data"
msgid "Some of the fields required for Quick Checkout activation are missing. Please check."
msgstr "Một số trường cần thiết để kích hoạt Quick Checkout đã bị thiếu. Vui lòng kiểm tra."

msgctxt "Languages::text_skrill_logo_notice"
msgid "If your HTTPS server is not set up, the logo will be passed by the HTTP protocol and users will see the \"Connection Partially Encrypted\" warning on the Skrill page."
msgstr "Nếu máy chủ HTTPS của bạn không được thiết lập, biểu tượng sẽ được thông qua bởi các giao thức HTTP và người dùng sẽ thấy cảnh báo \"Connection Partially Encrypted\" trên trang Skrill."

msgctxt "Languages::text_skrill_notice"
msgid "To gain access to the international payment network of Skrill, please register <a href=\"[register_url]\" target=\"_blank\">here</a> for a free account if you do not have one yet.<p><strong>Attention</strong>: please save settings before validating email, secret word or activating merchant tools.</p>"
msgstr "Để truy cập vào các mạng lưới thanh toán quốc tế Skrill, xin vui lòng đăng ký <a href=\"[register_url]\" target=\"_blank\"> ở đây</a> cho một tài khoản miễn phí nếu bạn không có được nêu ra. <p><strong>Chú ý</strong>: xin vui lòng lưu các thiết đặt trước khi phê chuẩn thư điện tử, từ bí mật hoặc kích hoạt các công cụ thương gia.</p>"

msgctxt "Languages::text_skrill_notice_register"
msgid "To gain access to the international payment network of Skrill, please register <a href=\"[register_url]\" target=\"_blank\">here</a> for a free account if you do not have one yet.<p><strong>Attention</strong>: please save settings before activating merchant tools.</p>"
msgstr ""

msgctxt "Languages::text_skrill_payment_is_not_saved"
msgid "In order to use this feature, please save the payment method first."
msgstr "Để sử dụng tính năng này, đầu tiên xin vui lòng lưu phương thức thanh toán."

msgctxt "Languages::text_skrill_secred_word_notice"
msgid "The secret word must be in lower case without special characters and at least 10 digits long."
msgstr "Từ bí mật phải ít hơn mà không có ký tự đặc biệt và ít nhất dài 10 chữ số."

msgctxt "Languages::text_skrill_secret_word_is_correct"
msgid "Specified secret word is correct."
msgstr "Từ bí mật đã chỉ định là chính xác."

msgctxt "Languages::text_skrill_secret_word_is_incorrect"
msgid "Your secret word is incorrect. Please choose your secret word in the Merchant Tools section of your Skrill Account and then enter it here."
msgstr "Từ bí mật của bạn là không chính xác. Hãy chọn từ bí mật của bạn trong phần Merchant Tools trên Tài khoản Skrill của bạn và sau đó nhập nó ở đây."

msgctxt "Languages::text_skrill_support"
msgid "<b>Support:</b> <br /><br /> Do you have questions?<br /> Contact Skrill on <a href=\"mailto:\">ecommerce@skrill.com</a> or by phone +44 (0) 870 383 0762.<br />"
msgstr "<b>Hỗ trợ:</b> <br /> < br / >Bạn có câu hỏi? <br />Liên hệ với Skrill <a href=\"mailto:\"> ecommerce@skrill.com</a> hoặc qua điện thoại + 44 (0) 870 383 0762. <br />"

msgctxt "Languages::text_status_is_float"
msgid "You cannot use time interval for the \"Status\" parameter because this parameter is time dependent."
msgstr "Bạn không thể sử dụng khoảng thời gian cho các tham số \"Trạng thái\" bởi vì tham số này là thời gian phụ thuộc."

msgctxt "Languages::text_storage_changed"
msgid "Storage changed successfully"
msgstr "Lưu trữ thay đổi thành công"

msgctxt "Languages::text_store_mode_changed_to_full"
msgid "<strong>Full mode</strong> has been activated. In this mode, all the advanced eCommerce features are available."
msgstr "<strong>Chế độ đầy đủ</strong> đã được kích hoạt. Trong chế độ này, tất cả các tính năng thương mại điện tử tiên tiến có sẵn."

msgctxt "Languages::text_store_mode_closed"
msgid "The store was <b>closed</b>"
msgstr "Cửa hàng đã <b>đóng cửa</b>"

msgctxt "Languages::text_store_mode_full"
msgid "<p>Full unrestricted access to all features. Enter a valid license number to activate.</p><p>This mode can be activated at any time.</p><br />"
msgstr "<p>Đầy đủ không bị giới hạn truy cập vào tất cả các tính năng. Nhập một số giấy phép hợp lệ để kích hoạt.</p> <p>Chế độ này có thể được kích hoạt bất cứ lúc nào.</p> <br />"

msgctxt "Languages::text_store_mode_opened"
msgid "The store was <b>opened</b>"
msgstr "Cửa hàng đã <b>mở</b>"

msgctxt "Languages::text_store_mode_open"
msgid "The store was <b>opened</b>"
msgstr ""

msgctxt "Languages::text_store_mode_trial"
msgid "<p>Full access to all features during the 30-day trial period, starting from the time of activation.</p><p>Once the 30-day trial period expires, you'll have to <a href=\"[product_buy_url]\" target=\"_blank\">purchase a license</a> or enter a valid license number to be able to manage your store.</p>"
msgstr "<p>Đầy đủ quyền truy cập vào tất cả các tính năng trong thời gian dùng thử 30 ngày, bắt đầu từ thời điểm kích hoạt.</p> <p>Sau khi thời gian dùng thử 30 ngày hết hạn, bạn sẽ phải <a href=\"[product_buy_url]\" target=\"_blank\"> mua một giấy phép</a> hoặc nhập một số hợp lệ giấy phép để có thể quản lý các cửa hàng của bạn.</p>"

msgctxt "Languages::text_store_mode_trial_rb"
msgid "All features are available during 30 days after download of the installation package. You don't need to enter the license key.<br /><br />After the 30-day trial period is over, you need to buy the license or enter the license key if you already have one.<br /><br />"
msgstr "Tất cả các tính năng có sẵn trong 30 ngày sau khi tải về gói cài đặt. Bạn không cần phải nhập các mã khóa giấy phép. <br /> <br /> Sau khi thời gian dùng thử 30 ngày hết hạn, bạn cần phải mua giấy phép hoặc nhập giấy phép chính thức nếu bạn đã có nó. <br /> <br />"

msgctxt "Languages::text_tax_applied"
msgid "The tax <b>[tax]</b> has been applied to all products"
msgstr "Thuế <b>[tax]</b> đã được áp dụng cho tất cả sản phẩm"

msgctxt "Languages::text_tax_unset"
msgid "The tax <b>[tax]</b> has been removed from all products"
msgstr "Thuế <b>[tax]</b> đã được gỡ bỏ từ tất cả sản phẩm"

msgctxt "Languages::text_template_changed"
msgid "Your changes have not been saved.Press OK to save changes or Cancel to discard them."
msgstr "Thay đổi của bạn chưa được lưu. Bấm OK để lưu thay đổi hoặc huỷ để bỏ chúng."

msgctxt "Languages::text_thaiepay_notice"
msgid "<p>Please set the following values in the Thaiepay merchant settings:</p><p>Return Url: <b>[return_url]</b></p><p>Status: <b>Yes (it is important!)</b></p><p>Post Back Url: <b>[postback_url]</b></p><p>Main Parameter: <b>\"total\" must be checked</b> - it will increase the security of transactions</p><p>Additional Parameter: it is obligatory for successful processing of transactions. For example, you can set <b>name=completed</b> and <b>value=some_secret_word</b>. Copy these values to these form fields: \"Additional parameter Name\" and \"Additional parameter Value\".</p>"
msgstr "<p>Xin vui lòng thiết lập các giá trị sau trong thiết lập buôn Thaiepay:</p> <p>Trở lại Url: <b>[return_url]</b></p> <p>Trạng thái: <b>Yes (nó là quan trọng!)</b></p> <p>Bài viết lại Url: <b>[postback_url]</b></p> <p>Tham số chính: <b>\"tất cả\" phải được kiểm tra</b> - nó sẽ làm tăng tính bảo mật của giao dịch</p> Tham số bổ sung <p>: nó là bắt buộc cho các xử lý thành công của các giao dịch. Ví dụ, bạn có thể thiết lập <b>tên = hoàn thành</b> và <b>giá trị = some_secret_word</b>. Sao chép các giá trị này vào các lĩnh vực hình thức: \"Tham số bổ sung tên\" và \"Tham số bổ sung giá trị\".</p>"

msgctxt "Languages::text_thumbnail_manual_loading"
msgid "Thumbnails will be generated from detailed images automatically, but you can also <a id=\"[id]\" class=\"[class]\">upload them manually.</a>"
msgstr "Hình thu nhỏ sẽ được tự động tạo ra từ hình ảnh chi tiết, nhưng bạn cũng có thể <a id=\"[id]\" class=\"[class]\"> tải chúng lên theo cách thủ công.</a>"

msgctxt "Languages::text_tinymce"
msgid "TinyMCE"
msgstr "TinyMCE"

msgctxt "Languages::text_topmenu_more"
msgid "More [item] <i class=\"text-arrow\">&rarr;</i>"
msgstr "Thêm [item] <i class=\"text-arrow\"> &rarr;</i>"

msgctxt "Languages::text_topmenu_view_more"
msgid "View more <i class=\"text-arrow\">&rarr;</i>"
msgstr "Xem thêm <i class=\"text-arrow\"> &rarr;</i>"

msgctxt "Languages::text_track_instructions_sent"
msgid "Access instructions were sent to your e-mail address."
msgstr "Hướng dẫn truy cập được gửi đến địa chỉ email của bạn."

msgctxt "Languages::text_track_request"
msgid "You requested information on the orders you placed in our store."
msgstr "Bạn yêu cầu thông tin về các đơn đặt hàng bạn đặt trong cửa hàng của chúng tôi."

msgctxt "Languages::text_track_view_all_orders"
msgid "To view all your orders, please follow this link:"
msgstr "Để xem tất cả các đơn đặt hàng của bạn, hãy làm theo liên kết này:"

msgctxt "Languages::text_track_view_order"
msgid "To view order #[order] please open this link:"
msgstr "Để xem đơn hàng #[order] xin vui lòng mở liên kết này:"

msgctxt "Languages::text_transaction_cancelled"
msgid "Transaction was canceled by the customer"
msgstr "Giao dịch bị hủy bỏ bởi khách hàng"

msgctxt "Languages::text_transaction_declined"
msgid "Transaction was declined by the payment processor or was canceled by the customer"
msgstr "Giao dịch đã bị từ chối bởi bộ vi xử lý thanh toán hoặc đã bị hủy bỏ bởi khách hàng"

msgctxt "Languages::text_uc_another_update_process_running"
msgid "Possibly another upgrade process is running.<br />Please wait until it is finished.<br />If the process has finished with an error, you can remove the file [filename] and try to restart the upgrade.<br /><a href=\"[url]\">Remove the lock file and restart the upgrade</a>"
msgstr "Có thể một quá trình nâng cấp là đang chạy. <br /> Xin vui lòng chờ cho đến khi nó được hoàn tất. <br /> Nếu quá trình kết thúc với một lỗi, bạn có thể loại bỏ tập tin [filename] và cố gắng để khởi động lại nâng cấp. <br /> <a href=\"[url]\"> Xóa tệp theo dõi và khởi động lại nâng cấp</a>"

msgctxt "Languages::text_uc_backup_database"
msgid "The following database tables have been backed up"
msgstr "Cơ sở dữ liệu bảng dưới đây đã được sao lưu"

msgctxt "Languages::text_uc_backup_files"
msgid "The following files have been backed up"
msgstr "Các tập tin sau đây đã được sao lưu"

msgctxt "Languages::text_uc_broken_package"
msgid "The upgrade package seems to be broken. Please refresh packages list and try to download it again"
msgstr "Các gói phần mềm nâng cấp có vẻ là bị hỏng. Xin vui lòng làm mới danh sách gói và cố gắng để tải về nó một lần nữa"

msgctxt "Languages::text_uc_cannot_lock_upgrade_process"
msgid "Failed to disable upgrade process launching"
msgstr "Không thể vô hiệu hóa quá trình nâng cấp được bung ra"

msgctxt "Languages::text_uc_cant_download_package"
msgid "Cannot download the upgrade package"
msgstr "Không thể tải về các gói nâng cấp"

msgctxt "Languages::text_uc_changed_files_message"
msgid "These files have local modifications and will be backed up. Once the upgrade is complete, compare the backup files with the new files and reapply the modifications to the new files, if necessary."
msgstr "Những tập tin đã thay đổi cục bộ và sẽ được sao lưu. Sau khi nâng cấp xong, so sánh các tập tin sao lưu với các tập tin mới và áp dụng lại những sửa đổi các tập tin mới, nếu cần thiết."

msgctxt "Languages::text_uc_check_ok"
msgid "Your [product] version has been checked successfully. It is ready for upgrade."
msgstr "Phiên bản [product] của bạn đã được kiểm tra thành công. Nó đã sẵn sàng cho nâng cấp."

msgctxt "Languages::text_uc_conflicts"
msgid "The following files had local modifications"
msgstr "Các tập tin sau đây đã sửa đổi cục bộ"

msgctxt "Languages::text_uc_db_right_needed"
msgid "Not enough permissions to update database. Please assign the following privileges to your database user [db_user]: [priviliges]."
msgstr "Không đủ quyền để cập nhật cơ sở dữ liệu. Xin vui lòng chỉ định các quyền sau đây để sử dụng cơ sở dữ liệu của bạn [db_user]: [priviliges]."

msgctxt "Languages::text_uc_emergency_restore"
msgid "Attention! Please copy and save the URL below - you can use it to restore your store if upgrade fails.<br /><b>[href]</b>"
msgstr "Chú ý! Xin vui lòng sao chép và lưu URL dưới đây - bạn có thể sử dụng nó để khôi phục lại cửa hàng của bạn nếu nâng cấp thất bại. <br /> <b>[href]</b>"

msgctxt "Languages::text_uc_failed_to_backup_tables"
msgid "The database tables have not been backed up"
msgstr "Các bảng cơ sở dữ liệu không được sao lưu"

msgctxt "Languages::text_uc_failed_to_create_directory"
msgid "Failed to create a new directory"
msgstr "Thất bại trong việc tạo ra một thư mục mới"

msgctxt "Languages::text_uc_failed_to_ftp_copy"
msgid "FTP transfer failed"
msgstr "Chuyển giao FTP không thành công"

msgctxt "Languages::text_uc_ftp_cart_directory_not_found"
msgid "We were unable to find CS-Cart installation in the specified directory. Please make sure that you have entered the correct FTP directory."
msgstr "Chúng tôi đã không thể tìm thấy CS-Cart cài đặt trong thư mục đã chỉ định. Hãy chắc chắn rằng bạn đã nhập các thư mục FTP chính xác."

msgctxt "Languages::text_uc_ftp_connection_failed"
msgid "FTP connection failed"
msgstr "Kết nối FTP không thành công"

msgctxt "Languages::text_uc_ftp_connect_failed"
msgid "Cannot connect to FTP server. Please check if the host name is correct"
msgstr "Không thể kết nối tới máy chủ FTP. Xin vui lòng kiểm tra nếu tên máy chủ lưu trữ là chính xác"

msgctxt "Languages::text_uc_ftp_login_failed"
msgid "Cannot log in to FTP server. Please check if username and password are correct"
msgstr "Không thể đăng nhập vào máy chủ FTP. Xin vui lòng kiểm tra nếu tên người dùng và mật khẩu là chính xác"

msgctxt "Languages::text_uc_ftp_needed"
msgid "FTP credentials to access the files and set proper permissions automatically"
msgstr "FTP thông tin đăng nhập để truy cập các tập tin và đặt đúng quyền tự động"

msgctxt "Languages::text_uc_has_conflicts"
msgid "Some of your files had local modifications. You can check them by clicking the following link"
msgstr "Một số tập tin đã thay đổi cục bộ. Bạn có thể kiểm tra chúng bằng cách nhấn vào liên kết sau"

msgctxt "Languages::text_uc_incorrect_upgrade_path"
msgid "Wrong path to upgrade files"
msgstr "Sai đường dẫn nâng cấp tập tin"

msgctxt "Languages::text_uc_license_number_required"
msgid "Please enter your [product] license number"
msgstr "Vui lòng nhập số giấy phép [product] của bạn"

msgctxt "Languages::text_uc_list_of_updates_missing"
msgid "List of upgrades is missing"
msgstr "Danh sách các nâng cấp bị mất tích"

msgctxt "Languages::text_uc_non_writable_files"
msgid "These files require write permissions set (manually or automatically via FTP)"
msgstr "Các tập tin yêu cầu thiết lập quyền ghi (bằng tay hoặc tự động thông qua FTP)"

msgctxt "Languages::text_uc_no_enough_space_to_backup_database"
msgid "Insufficient disk space to back up the database"
msgstr "Không đủ bộ nhớ trống để sao lưu cơ sở dữ liệu"

msgctxt "Languages::text_uc_no_ftp_module"
msgid "It seems that no FTP module is installed on your server. You cannot use FTP until this module is installed"
msgstr "Nó có vẻ rằng không có mô-đun FTP được cài đặt trên máy chủ của bạn. Bạn không thể sử dụng FTP cho đến khi mô-đun này được cài đặt"

msgctxt "Languages::text_uc_unable_to_create_upgrade_folder"
msgid "Failed to create an upgrade folder"
msgstr "Thất bại trong việc tạo ra một thư mục nâng cấp"

msgctxt "Languages::text_uc_unable_to_parse_uc_xml"
msgid "Failed to parse the upgrade process descriptor uc.xml"
msgstr "Thất bại trong việc phân tích cú pháp uc.xml mô tả quá trình nâng cấp"

msgctxt "Languages::text_uc_unable_to_remove_file"
msgid "Failed to remove the file"
msgstr "Thất bại trong việc loại bỏ tập tin"

msgctxt "Languages::text_uc_unable_to_remove_packages_xml"
msgid "Failed to remove the upgrade descriptor packages.xml"
msgstr "Thất bại trong việc loại bỏ mô tả nâng cấp packages.xml"

msgctxt "Languages::text_uc_unable_to_remove_upgrade_lock"
msgid "Failed to remove the upgrade lock file. Please remove the [file] file."
msgstr "Thất bại trong việc loại bỏ tập tin theo dõi nâng cấp. Hãy loại bỏ các tập tin [file]."

msgctxt "Languages::text_uc_unable_to_update_list_of_installed_upgrades"
msgid "Failed to update the list of installed upgrades"
msgstr "Thất bại trong việc cập nhật danh sách các cài đặt nâng cấp"

msgctxt "Languages::text_uc_upgrade_completed"
msgid "Your store has been upgraded successfully"
msgstr "Cửa hàng của bạn đã được nâng cấp thành công"

msgctxt "Languages::text_uc_upgrade_completed_check_and_open"
msgid "<p>Your store was closed during the upgrading procedure. We recommend you to check the work of the store after the upgrade and only then open it.</p><p>You can open the store by unchecking the Close storefront setting under Settings → General.</p>"
msgstr "<p>Cửa hàng của bạn đã được đóng để nâng cấp. Chúng tôi đề nghị bạn kiểm tra công việc của các cửa hàng sau khi nâng cấp và chỉ sau đó mở nó</p> <p>Bạn có thể mở các cửa hàng bằng cách bỏ tích chọn mục thiết lập Đóng cửa hàng trong Cài đặt → Chung.</p>"

msgctxt "Languages::text_uc_upgrade_log_file_not_writable"
msgid "Upgrade log file is not writable"
msgstr "Nâng cấp các tập tin nhật ký không thể ghi được"

msgctxt "Languages::text_uc_upgrade_needed"
msgid "You need to upgrade to version <b>[to_version]</b> before applying this upgrade package (your version is <b>[your_version]</b>)."
msgstr "Bạn cần phải nâng cấp lên phiên bản <b>[to_version]</b> trước khi áp dụng gói nâng cấp này (Phiên bản của bạn là <b>[your_version]</b>)."

msgctxt "Languages::text_uc_upgrade_not_selected"
msgid "No upgrade package is selected"
msgstr "Không có gói nâng cấp được chọn"

msgctxt "Languages::text_uc_upgrade_reverted"
msgid "The upgrade has been reverted successfully"
msgstr "Việc nâng cấp đã được chuyển thành công"

msgctxt "Languages::text_ult_product_store_field_tooltip"
msgid "Product owner. Only product owner can edit all fields in shared products."
msgstr "Chủ sở hữu sản phẩm. Chỉ chủ sở hữu sản phẩm có thể chỉnh sửa tất cả các trường trong các sản phẩm được chia sẻ."

msgctxt "Languages::text_unable_to_parse_xml"
msgid "Failed to parse XML structure"
msgstr "Thất bại trong việc phân tích cú pháp XML cấu trúc"

msgctxt "Languages::text_unsupported_currency"
msgid "The currency which is not supported by the payment system is used on the website. Please contact the store administrator regarding this issue."
msgstr "Loại tiền tệ không được hỗ trợ bởi hệ thống thanh toán đâng sử dụng trên trang web. Xin vui lòng liên hệ với người quản lý cửa hàng liên quan đến vấn đề này."

msgctxt "Languages::text_upgrade_available"
msgid "A new version of [product] is available. If you want to upgrade to the latest version now, click <a href=\"[link]\">here</a>."
msgstr "Một phiên bản mới của [product] có sẵn. Nếu bạn muốn nâng cấp lên phiên bản mới nhất bây giờ, hãy nhấp vào <a href=\"[link]\"> đây</a>."

msgctxt "Languages::text_usergroup_activated"
msgid "The following user groups have been activated for your account: [usergroups]."
msgstr "Nhóm người dùng sau đây đã được kích hoạt cho tài khoản của bạn: [usergroups]."

msgctxt "Languages::text_usergroup_disactivated"
msgid "The following user groups have been removed from your account: [usergroups]."
msgstr "Nhóm người dùng sau đây đã được gỡ bỏ từ tài khoản của bạn: [usergroups]."

msgctxt "Languages::text_usergroup_request"
msgid "Customer requested user group."
msgstr "Khách hàng yêu cầu nhóm người dùng."

msgctxt "Languages::text_usergroup_requested_by"
msgid "The <b>[usergroup]</b> group was requested by [name] ([email])."
msgstr "Nhóm <b>[usergroup]</b> được yêu cầu bởi [name] ([email])."

msgctxt "Languages::text_worldpay_notice"
msgid "Configure your WorldPay account before using the payment.<br /><br />Go to the WorldPay administration page and switch to the Installation section.<br /><br />Fill the fields as follows:<br /><br />Payment Response URL: <b>[return_url]</b><br />Payment Response Password: <b>Make up a secret word.</b> The same word must be entered in the respective field below<br />MD5 secret for transactions: <b>Make up a secret word.</b> The same word must be entered in the respective field below<br />SignatureFields: <b>instId:amount:currency:cartId</b>"
msgstr "Cấu hình tài khoản WorldPay của bạn trước khi sử dụng thanh toán. <br /> <br />Đi tới trang quản trị WorldPay và chuyển sang phần cài đặt. <br /> <br /> Điền vào các trường như sau: <br /> <br /> URL Phản hồi Thanh toán: <b>[return_url]</b> <br /> Mật khẩu Thanh toán Phản hồi: <b>Tạo một từ khóa bí mật.</b> Cùng một từ đó phải được nhập trong lĩnh vực tương ứng dưới đây <br /> MD5 bí mật cho các giao dịch: <b>Tạo một từ khóa bí mật.</b> Cùng một từ đó phải được nhập trong lĩnh vực tương ứng dưới đây <br /> SignatureFields: <b>instId:amount:currency:cartId</b>"

msgctxt "Languages::text_zipcodes_wildcards"
