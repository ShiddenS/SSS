msgid "Unknown server response"
msgstr "Phản ứng máy chủ không xác định"

msgctxt "Languages::unmark"
msgid "Unmark"
msgstr "Bỏ đánh dấu"

msgctxt "Languages::unregistered_customer"
msgid "Unregistered customer"
msgstr "Khách hàng chưa đăng ký"

msgctxt "Languages::unregistered_customer_short"
msgid "Unregistered"
msgstr ""

msgctxt "Languages::unselect_all"
msgid "Unselect all"
msgstr "Bỏ chọn tất cả"

msgctxt "Languages::unset_tax_to_products"
msgid "Remove selected taxes from all products"
msgstr "Loại bỏ thuế được chọn từ tất cả sản phẩm"

msgctxt "Languages::unsubscribe"
msgid "Unsubscribe"
msgstr "Bỏ đăng ký"

msgctxt "Languages::up"
msgid "Up"
msgstr "Lên"

msgctxt "Languages::update"
msgid "Update"
msgstr "Cập Nhật"

msgctxt "Languages::update_categories"
msgid "Update categories"
msgstr "Cập nhật danh mục"

msgctxt "Languages::update_customer_info"
msgid "Update customer information"
msgstr "Cập nhật thông tin khách hàng"

msgctxt "Languages::update_for_all_act"
msgid "New value will be saved for ALL stores which share this item. Click to save it only for the owner-store."
msgstr "Giá trị mới sẽ được lưu cho tất cả các cửa hàng mà chia sẻ khoản mục này. Bấm vào để lưu nó chỉ dành cho các chủ sở hữu cửa hàng."

msgctxt "Languages::update_for_all_dis"
msgid "New value will be saved ONLY for the owner-store. Click to save it for all stores which share this item."
msgstr "Giá trị mới sẽ được lưu CHỈ cho chủ sở hữu cửa hàng. Bấm vào để tiết kiệm nó cho tất cả các cửa hàng mà chia sẻ khoản mục này."

msgctxt "Languages::update_for_all_hid_act"
msgid "Click to cancel field update."
msgstr "Bấm để hủy bỏ trường cập nhật."

msgctxt "Languages::update_for_all_hid_dis"
msgid "Click to enter a new field value to update in all stores when saving item."
msgstr "Bấm để nhập một giá trị trường mới để cập nhật trong tất cả các cửa hàng khi lưu mục."

msgctxt "Languages::update_group"
msgid "Update group"
msgstr "Cập nhật nhóm"

msgctxt "Languages::update_period_expired"
msgid "<p class=\"text-error\">Your upgrade subscription has expired, so this software upgrade cannot be installed. You can prolongate your subscription in the <a href=\"[url]\">Customer Help Desk</a>.</p><p>Please note that it is not mandatory: you can continue using this software with no limitation even without an upgrade subscription.</p>"
