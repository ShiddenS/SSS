msgid "Provider"
msgstr "Сервис"

msgctxt "SettingsVariants::geo_maps::provider::google"
msgid "Google"
msgstr "Google"

msgctxt "SettingsVariants::geo_maps::provider::yandex"
msgid "Yandex"
