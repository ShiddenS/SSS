msgctxt "Addons::name::rus_kupivkredit"
msgid "Russian localization — Kupi V Kredit"
msgstr "Купи В Кредит"

msgctxt "Addons::description::rus_kupivkredit"
msgid "Allows purchases on credit via the Kupi V Kredit service"
msgstr "Модуль позволяющий покупать продукты в кредит через систему Купи В Кредит"

msgctxt "Languages::kvk_order_id"
msgid "Kupi v kredit order id"
msgstr "Номер заказа в системе Купи в кредит"

msgctxt "Languages::kupivkredit_shop_id"
msgid "Store ID"
msgstr "Идентификатор магазина"

msgctxt "Languages::rus_kupivkredit.show_case_id"
msgid "Showcase ID"
msgstr "Идентификатор витрины магазина"

msgctxt "Languages::kupivkredit_api_key"
msgid "API Key"
msgstr "API ключ"

msgctxt "Languages::kupivkredit_secret"
msgid "Secret word"
msgstr "Секретная строка"

msgctxt "Languages::kupivkredit_button"
msgid "Kupi v kredit"
msgstr "Купи в кредит"

msgctxt "Languages::kupivkredit"
msgid "Kupi V Kredit"
msgstr "Купи в кредит"

msgctxt "Languages::kupivkredit_widget_closed"
msgid "The widget has been closed by the customer"
msgstr "Виджет был закрыт покупателем"

msgctxt "Languages::kupivkredit_cancel_order"
msgid "Cancel order"
msgstr "Купи в кредит - Отменить заказ"

msgctxt "Languages::kupivkredit_complete_order"
msgid "Sign order"
msgstr "Купи в кредит - Подписать заказ"

msgctxt "Languages::kupivkredit_confirm_order"
msgid "Confirn order"
msgstr "Купи в кредит - Подтвердить заказ"

msgctxt "Languages::kupivkredit_cancel_order_status"
msgid "Declined by the store"
msgstr "Отказ магазина"

msgctxt "Languages::kupivkredit_widget_incomplete"
msgid "The request is incomplete"
msgstr "Заявка не заполнена до конца"

msgctxt "Languages::kupivkredit_widget_bank_reject"
msgid "Request declined by bank"
msgstr "Заявка отклонена банком"

msgctxt "Languages::kupivkredit_widget_open"
msgid "Your request is pre-approved"
msgstr "Ваша заявка предварительно одобрена"

msgctxt "Languages::kupivkredit_widget_processed"
msgid "Congratulations! Your request is approved."
msgstr "Ваша заявка одобрена! Поздравляем!"

msgctxt "Languages::kupivkredit_order_confirmed"
msgid "Order confirmation sent to bank"
msgstr "В банк отправлено подтверждение заказа"

msgctxt "Languages::rus_kupivkredit.settings_instructions"
msgid "You can find detailed information about registration in Kupi V Kredit system on the <a href=\"[url]\">\"[url]\"</a>"
msgstr "Подробнее о регистрации в системе \"Купи в кредит\" читайте по ссылке: <a href=\"[url]\">\"[url]\"</a>"

msgctxt "Languages::kvk_payment_disabled"
msgid "Some payments use the Kupi V Kredit processor. They have been disabled and cannot be activated unless the Kupi V Kredit processor is active."
msgstr "Методы оплаты, использующие модуль Купи в кредит, были отключены. Для того чтобы их включить, необходимо, чтобы модуль Купи в кредит был активен."

msgctxt "Languages::kvk_has_disabled_payments"
msgid "Some payments use Kupi V Kredit processor. You can now activate them on the <a href=\"[url]\">Payment methods</a> page."
msgstr "Некоторые методы оплаты использут модуль Купи в кредит. Вы можете активировать их на странице <a href=\"[url]\">Методы оплаты</a>."

msgctxt "Languages::rus_kupivkredit.redirect_to_create_order"
msgid "Now you will be redirected to the site of the payment system."
msgstr "Теперь вы будете перенаправлены на сайт платёжной системы."

