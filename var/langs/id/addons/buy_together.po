msgctxt "Addons::name::buy_together"
msgid "Buy together"
msgstr "Membeli bersama-sama"

msgctxt "Addons::description::buy_together"
msgid "Allows you to set up share discounts on certain product combinations"
msgstr "Memungkinkan Anda untuk mengatur berbagi diskon kombinasi produk tertentu"

msgctxt "Languages::buy_together"
msgid "Buy together"
msgstr "Membeli bersama-sama"

msgctxt "Languages::specify_options"
msgid "Specify options"
msgstr "Spacify options"

msgctxt "Languages::total_list_price"
msgid "Total list price"
msgstr "Total daftar harga"

msgctxt "Languages::price_for_all"
msgid "Price for all"
msgstr "Harga untuk semua"

msgctxt "Languages::add_all_to_cart"
msgid "Add all to cart"
msgstr "Tambahkan semua ke keranjang"

msgctxt "Languages::editing_combination"
msgid "Editing combination"
msgstr "Mengedit kombinasi"

msgctxt "Languages::buy_together_calculation_information"
msgid "Total price: [total_price], Discount: [discount], Combination price: [combination_price]"
msgstr "Total Harga: [total_price], diskon: [discount], kombinasi Harga: [combination_price]"

msgctxt "Languages::display_in_promotions"
msgid "Display in promotions"
msgstr "Dalam promosi"

msgctxt "Languages::combination_products"
msgid "Combination products"
msgstr "Kombinasi produk"

msgctxt "Languages::share_discount"
