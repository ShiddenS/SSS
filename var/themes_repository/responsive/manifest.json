            "thumbnails_gallery": true,
            "enable_quick_view": false,
            "available_product_list_sortings": [
                "product-asc",
                "product-desc",
                "price-asc",
                "price-desc"
            ]
        },
        "Thumbnails": {
            "product_lists_thumbnail_width": 300,
            "product_lists_thumbnail_height": 300
        }
    }
